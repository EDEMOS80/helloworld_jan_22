pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello '
            }
        }
        stage('Build'){
            steps{
            echo ' Build'
            }
        }
         stage('test'){
            steps{
            echo 'test'
    }
}
  stage('deploy'){
           steps{
            echo 'deploy'
            }
        }
    }
}   
